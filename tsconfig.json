{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "lib": ["es2018", "dom"],
    "typeRoots": ["node_modules/@types"],
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "paths": {
      "objects-angular-client": [
        "dist/objects-angular-client/objects-angular-client",
        "dist/objects-angular-client"
      ],
      "objects-angular-forms": [
        "dist/objects-angular-forms/objects-angular-forms",
        "dist/objects-angular-forms"
      ]
    }
  },
  "angularCompilerOptions": {
    "fullTemplateTypeCheck": true,
    "strictInjectionParameters": true
  },
  "paths": {
    "json-schema-ref-parser": [
      "node_modules/json-schema-ref-parser/dist/ref-parser.js"
    ]
  }
}
